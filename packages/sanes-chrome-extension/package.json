{
  "name": "sanes-chrome-extension",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "oza-react-scripts build",
    "format": "prettier --write --loglevel warn './src/**/*.{ts,tsx,json,md,css}'",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts{,x}'",
    "start": "oza-react-scripts start",
    "test": "CI=true oza-react-scripts test --env=jsdom",
    "watch": "cra-build-watch --cs src/extension/contentscript.ts --bs src/extension/backgroundscript.ts"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@iov/core": "^0.13.0",
    "@iov/crypto": "^0.13.0",
    "@iov/keycontrol": "^0.13.0",
    "@types/chrome": "^0.0.81",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.6",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "connected-react-router": "^6.3.1",
    "encoding-down": "^6.0.1",
    "history": "^4.7.2",
    "level-js": "^4.0.0",
    "medulas-react-components": "*",
    "memdown": "^3.0.0",
    "oza-react-scripts": "^0.0.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^3.2.1",
    "webext-redux": "^2.0.1"
  },
  "devDependencies": {
    "@types/encoding-down": "^5.0.0",
    "@types/levelup": "^3.1.0",
    "@types/memdown": "^3.0.0",
    "cra-build-watch": "ssh://git@github.com:apanizo/cra-build-watch#a768c63d7deb0d56754426b0071a85562d0d9509",
    "jest": "23.6.0",
    "ts-jest": "23.10.5"
  }
}
